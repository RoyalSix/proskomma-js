<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns:table="urn:oasis:names:tc:opendocument:xmlns:table:1.0"
      xmlns:text="urn:oasis:names:tc:opendocument:xmlns:text:1.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns="http://www.crosswire.org/2008/TEIOSIS/namespace"
      xsi:schemaLocation="http://www.crosswire.org/2013/TEIOSIS/namespace  http://www.crosswire.org/OSIS/teiP5osis.2.5.0.xsd">
      <!-- Downloaded from https://github.com/biblicalhumanities/Dodson-Greek-Lexicon on 10th November 2020 -->
      <entry n="ἄλφα | 0001">
            <orth>ἄλφα</orth>
            <def role="brief">the first letter of the Greek alphabet</def>
            <def role="full">alpha; the first letter of the Greek alphabet.</def>
      </entry>
      <entry n="Ἀαρών | 0002">
            <orth>Ἀαρών, ὁ</orth>
            <def role="brief">Aaron</def>
            <def role="full">Aaron, son of Amram and Jochebed, brother of Moses.</def>
      </entry>
      <entry n="Ἀβαδδών | 0003">
            <orth>Ἀβαδδών, ὁ</orth>
            <def role="brief">Abaddon</def>
            <def role="full">Abaddon, Destroyer (i.e. Destroying Angel) or "place of destruction"
                  (personified).</def>
      </entry>
      <entry n="ἀβαρής | 0004">
            <orth>ἀβαρής, ες</orth>
            <def role="brief">not burdensome</def>
            <def role="full">not burdensome, bringing no weight or oppression upon.</def>
      </entry>
      <entry n="Ἀββά | 0005">
            <orth>Ἀββά</orth>
            <def role="brief">Father</def>
            <def role="full">Abba, Father.</def>
      </entry>
      <entry n="Ἄβελ | 0006">
            <orth>Ἄβελ, ὁ</orth>
            <def role="brief">Abel</def>
            <def role="full">Abel, second son of Adam and Eve, brother of Cain.</def>
      </entry>
      <entry n="Ἀβιά | 0007">
            <orth>Ἀβιά, ὁ</orth>
            <def role="brief">Abijah</def>
            <def role="full">Abijah, (a) a king, son of Rehoboam, (b) founder of the eighth class of
                  priests.</def>
      </entry>
      <entry n="Ἀβιαθάρ | 0008">
            <orth>Ἀβιαθάρ, ὁ</orth>
            <def role="brief">Abiathar</def>
            <def role="full">Abiathar, a priest in King David's time.</def>
      </entry>
      <entry n="Ἀβιληνή | 0009">
            <orth>Ἀβιληνή, ῆς, ἡ</orth>
            <def role="brief">Abilene</def>
            <def role="full">the Abilenian territory, the territory of Abila (in Syria), a small
                  principality in the mountains of Damascus.</def>
      </entry>
</TEI>
